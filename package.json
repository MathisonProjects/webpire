{
  "name": "webpire",
  "version": "1.0.0",
  "description": "Quick site ready for development",
  "main": "index.js",
  "scripts": {
    "npm-install": "npm install",
    "dev": "cross-env rm -rf dist && npm run development",
    "nodeServer": "cross-env node ./node/server.js",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dockerDeploy": "npm run dockerDeployDist && npm run dockerDeployNode && npm run seedDynamo &&  npm run dockerDeployDynamoAdmin",
    "dockerDeployDist": "http-server dist -p 8081 &",
    "dockerDeployNode": "pm2 start ./node/server.js --no-daemon &",
    "dockerDeployDynamoAdmin": "DYNAMO_ENDPOINT=http://172.18.0.2:8000 dynamodb-admin",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "cross-env NODE_ENV=test node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-test": "npm run test -- --watch",
    "watch-test-poll": "npm run watch-test -- --watch-poll",
    "staging": "cross-env NODE_ENV=staging node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-staging": "npm run staging -- --watch",
    "watch-staging-poll": "npm run watch-staging -- --watch-poll",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-prod": "npm run prod -- --watch",
    "watch-prod-poll": "npm run watch-prod -- --watch-poll",
    "git-force-pull": "git fetch --all && git reset --hard origin/dev",
    "git-quick-push": "git add . && git commit -m \"Quick push\" && git push -u origin dev",
    "cleanHotUpdate": "cross-env FUNCFIRE=cleanHotUpdate node ./scripts.js",
    "updateVersion": "cross-env FUNCFIRE=updateVersion node ./scripts.js",
    "seedDynamo": "cross-env FUNCFIRE=seedDynamo node ./scripts.js &",
    "resetSeed": "cross-env FUNCFIRE=resetSeed node ./scripts.js"
  },
  "author": "Jacob Mathison",
  "license": "ISC",
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.14.2",
    "@types/socket.io-client": "^1.4.34",
    "@types/webpack-env": "^1.15.3",
    "aws-sdk": "^2.778.0",
    "cross-env": "^7.0.2",
    "dotenv-webpack": "^4.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "laravel-mix": "^5.0.7",
    "lodash": "^4.17.20",
    "path": "^0.12.7",
    "pm2": "^4.5.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1",
    "ts-loader": "^8.0.6",
    "typescript": "^4.0.3",
    "uuidv4": "^6.2.4",
    "vue": "^2.6.12",
    "vue-i18n": "^8.22.1",
    "vue-router": "^3.4.7",
    "vuetify": "^2.3.15",
    "vuex": "^3.5.1",
    "vuex-persistedstate": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.27.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12"
  }
}
